server {
    listen 80;
    server_name jozacsurabaya.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/static/;
    }

    location /media/ {
        root /home/static/;
    }

    location / {
        proxy_pass http://jozacsurabaya.com;
    }
}