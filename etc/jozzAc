upstream django{
    server unix:/home/demo/var/sock/gunicorn.sock;
}

server {
    listen 80;
    server_name jozacsurabaya.com;
    error_log /home/demo/var/logs/nginx.error.log warn;
    access_log /home/demo/var/logs/nginx.access.log;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/static/;
    }

    location /media/ {
        root /home/media/;
    }

    location / {
        include proxy_params;
        proxy_pass http://django/;
    }
}