server {
    listen 80;
    listen [::]:80;
    server_name jozacsurabaya.com http://jozacsurabaya.com/ www.jozacsurabaya.com;
    error_log /home/demo/var/logs/jozzAc.error.log warn;
    access_log /home/demo/var/logs/jozzAc.access.log;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/static/;
    }

    location /media/ {
        root /home/media/;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/demo/var/sock/gunicorn.sock;
    }
}