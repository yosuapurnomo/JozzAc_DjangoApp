[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=harry
Group=www-data
WorkingDirectory=/root/demo/jozzAC
ExecStart=/root/demo/virt/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/root/demo/var/gunicorn.sock \
          jozzAc.wsgi:application

[Install]
WantedBy=multi-user.target