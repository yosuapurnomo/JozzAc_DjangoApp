server {
    listen 80;
    server_name jozacsurabaya.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /root/JozzAc/jozzAC/staticfiles/;
    }
    location /media/ {
        root /root/JozzAc/jozzAC;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/root/JozzAc/jozzAC/myproject.sock;
    }
}