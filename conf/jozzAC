server {
    listen 80;
    server_name jozacsurabaya.com;

    location = /favicon.ico { access_log off; log_not_found off; }    location /static/ {
        alias /root/JozzAc/jozzAC;
    }
    location /media/ {
        alias /root/JozzAc/jozzAC;
    }

    location /shares {
            root /data/minio/data;
            autoindex on;
    }

    location /nginx {
            root /usr/share/nginx/html;
            autoindex on;
            index index.html index.htm;
    }

    location / {
        include proxy_params;
        uwsgi_pass unix://root/JozzAc/jozzAC/jozzAC.sock;
        proxy_pass http://unix:/root/JozzAc/jozzAC/jozzAC.sock;
        proxy_http_version 1.1;
        proxy_set_header   Upgrade $http_upgrade;
        proxy_set_header   Connection keep-alive;
        proxy_set_header   Host $host;
        proxy_cache_bypass $http_upgrade;
        proxy_set_header   X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header   X-Forwarded-Proto $scheme;
    }
}